{
    "version": 2,
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/main.py",
            "headers": { 
                "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload" 
            }
        }
    ]
}
